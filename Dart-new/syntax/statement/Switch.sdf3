module statement/Switch

imports
  
  Common
  Expression

context-free syntax
  
  // TODO: case and default must be prefixed with label*
  Label.Label = <<ID>:>
  
  Statement.Switch = <switch (<Expression>) {
    <{SwitchCase "\n"}*>
    <DefaultCase?>
  }>
  
  SwitchCase.SwitchCase = <<{Label " "}*> case <Expression>:
    <{Statement "\n"}*>>
  
  DefaultCase.DefaultCase = <<{Label " "}*> default:
    <{Statement "\n"}*>>
  